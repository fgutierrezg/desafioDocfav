{
    "name": "frnacisco/gestion-usuarios",
    "description": "Prueba Técnica PHP Gestión Usuarios",
    "type": "project",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
